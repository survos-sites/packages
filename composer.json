{
    "type": "project",
    "license": "proprietary",
    "name": "survos-sites/packages",
    "description": "Symfony application showing Symfony bundles",
    "keywords": ["symfony","pwa","datatables"],
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "adbario/php-dot-notation": "^3.3",
        "api-platform/core": "*",
        "cebe/php-openapi": "^1.8",
        "composer/semver": "^3.4",
        "doctrine/doctrine-bundle": "^2.12|^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.5",
        "doctrine/orm": "^3.4",
        "easycorp/easyadmin-bundle": "^4.27",
        "friendsofsymfony/jsrouting-bundle": "^3.5",
        "inspector-apm/inspector-symfony": "^1.2",
        "jwage/phpamqplib-messenger": "^0.8.3",
        "knplabs/knp-time-bundle": "^2.2",
        "knplabs/packagist-api": "dev-master",
        "meilisearch/meilisearch-php": "dev-main",
        "nadar/php-composer-reader": "^2.0",
        "nelmio/cors-bundle": "^2.4",
        "nyholm/psr7": "^1.8.1",
        "openconext/monitor-bundle": "^4.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "sentry/sentry-symfony": "^5.0.1",
        "survos/auth-bundle": "^2.0",
        "survos/bootstrap-bundle": "^2.0",
        "survos/command-bundle": "^2.0",
        "survos/core-bundle": "^2.0",
        "survos/crawler-bundle": "^2.0",
        "survos/installer": "^2.1",
        "survos/js-twig-bundle": "^2.0",
        "survos/lingua-core": "^2.0",
        "survos/meili-bundle": "^2.0",
        "survos/simple-datatables-bundle": "^2.0",
        "survos/state-bundle": "^2.0",
        "symfony/amqp-messenger": "^7.3",
        "symfony/asset": "^7.0.3",
        "symfony/asset-mapper": "^7.0.5",
        "symfony/console": "^7.0.4",
        "symfony/doctrine-messenger": "^7.0.4",
        "symfony/dotenv": "^7.0.4",
        "symfony/expression-language": "^7.0.3",
        "symfony/flex": "^2.4.5",
        "symfony/form": "^7.0.4",
        "symfony/framework-bundle": "^7.0.4",
        "symfony/http-client": "^7.0.5",
        "symfony/intl": "^7.0.3",
        "symfony/mailer": "^7.0.4",
        "symfony/mime": "^7.0.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.0.3",
        "symfony/object-mapper": "*",
        "symfony/process": "^7.0.4",
        "symfony/property-access": "^7.0.4",
        "symfony/property-info": "^7.0.3",
        "symfony/runtime": "^7.0.3",
        "symfony/security-bundle": "^7.0.5",
        "symfony/serializer": "^7.0.4",
        "symfony/stimulus-bundle": "^2.16",
        "symfony/string": "^7.0.4",
        "symfony/translation": "^7.0.4",
        "symfony/twig-bundle": "^7.0.4",
        "symfony/ux-chartjs": "^2.31",
        "symfony/ux-icons": "^2.18",
        "symfony/ux-turbo": "^2.16",
        "symfony/validator": "^7.0.5",
        "symfony/web-link": "^7.0.3",
        "symfony/yaml": "^7.0.3",
        "twig/extra-bundle": "^3.8",
        "twig/twig": "^3.17",
        "zenstruck/console-extra": "^1.4",
        "zenstruck/messenger-monitor-bundle": "^0.5.4",
        "zenstruck/twig-service-bundle": "^1.5"
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default  \"": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "setup": [
            "php -d memory_limit=4048M bin/console app:load-data"
        ],
        "load": [
            "php -d memory_limit=4048M bin/console workflow:iterate App\\\\Entity\\\\Package --marking=new --transition=load"
        ],
        "index": [
            "php -d memory_limit=2048M bin/console meili:index --reset"
        ],
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage"

    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.4"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "friendsofphp/php-cs-fixer": "^3.65",
        "nette/php-generator": "^4.1",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^12.1",
        "pierstoval/smoke-testing": "*",
        "rector/rector": "^2.0",
        "survos/code-bundle": "dev-main",
        "symfony/browser-kit": "^7.0.3",
        "symfony/css-selector": "^7.0.3",
        "symfony/debug-bundle": "^7.0.3",
        "symfony/maker-bundle": "^1.57",
        "symfony/phpunit-bridge": "^7.0.4",
        "symfony/stopwatch": "^7.0.3",
        "symfony/web-profiler-bundle": "^7.0.4"
    }
}
